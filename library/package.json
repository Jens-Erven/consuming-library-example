{
  "name": "@portima/fe-lib",
  "version": "0.0.1",
  "description": "A React component library with TypeScript and Tailwind CSS",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./dist/fe-lib.css",
    "./tokens/ocean/light": "./design-system/output/theme-ocean/light/ts/tokens.ts",
    "./tokens/ocean/dark": "./design-system/output/theme-ocean/dark/ts/tokens.ts",
    "./tokens/sunset/light": "./design-system/output/theme-sunset/light/ts/tokens.ts",
    "./tokens/sunset/dark": "./design-system/output/theme-sunset/dark/ts/tokens.ts",
    "./tokens/forest/light": "./design-system/output/theme-forest/light/ts/tokens.ts",
    "./tokens/forest/dark": "./design-system/output/theme-forest/dark/ts/tokens.ts",
    "./css/ocean/light": "./design-system/output/theme-ocean/light/css/tokens.css",
    "./css/ocean/dark": "./design-system/output/theme-ocean/dark/css/tokens.css",
    "./css/sunset/light": "./design-system/output/theme-sunset/light/css/tokens.css",
    "./css/sunset/dark": "./design-system/output/theme-sunset/dark/css/tokens.css",
    "./css/forest/light": "./design-system/output/theme-forest/light/css/tokens.css",
    "./css/forest/dark": "./design-system/output/theme-forest/dark/css/tokens.css",
    "./css/tailwind": "./design-system/output/tailwind/app.css"
  },
  "files": [
    "dist",
    "design-system/output",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run flatten:tokens && npm run build:variables && npm run build:tailwind && tsc && vite build",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "flatten:tokens": "node ./design-system/scripts/flatten-tokens.js",
    "build:variables": "node ./design-system/scripts/build-multi-themes.js",
    "build:tailwind": "node ./design-system/scripts/build-tailwind-css.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "react",
    "component-library",
    "typescript",
    "tailwind",
    "tailwindcss",
    "ui-components"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/component-library.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/component-library/issues"
  },
  "homepage": "https://github.com/your-username/component-library#readme",
  "peerDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.6",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    },
    "@mui/material": {
      "optional": false
    },
    "@emotion/react": {
      "optional": false
    },
    "@emotion/styled": {
      "optional": false
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "sideEffects": [
    "*.css",
    "dist/*.css"
  ],
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.6",
    "@storybook/addon-a11y": "^10.1.9",
    "@storybook/addon-docs": "^10.1.9",
    "@storybook/addon-themes": "^10.1.9",
    "@storybook/addon-vitest": "^10.1.9",
    "@storybook/react-vite": "^10.1.9",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.0.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.23",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "storybook": "^10.1.9",
    "style-dictionary": "^5.1.1",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.16"
  }
}
